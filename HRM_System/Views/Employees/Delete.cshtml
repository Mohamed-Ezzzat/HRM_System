@model Employee
@{
ViewData["Title"] = "Delete";
}


<h1>Are you Sure to Delete This Employee</h1>
<form asp-action="Delete">
<input hidden asp-for="Id"/>

    <div class="form-group">
        <label asp-for="Name" class="col-form-label"></label>
        <input asp-for="Name" class="form-control" readonly />
    </div> 
    <div class="form-group">
        <label asp-for="Deparment" class="col-form-label"></label>
        <input asp-for="Deparment" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label asp-for="Address" class="col-form-label"></label>
        <input asp-for="Address" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label asp-for="PhoneNumber" class="col-form-label"></label>
        <input asp-for="PhoneNumber" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label asp-for="Gender" class="col-form-label"></label>
        <input asp-for="Gender" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label asp-for="Nationality" class="col-form-label"></label>
        <input asp-for="Nationality" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label asp-for="Age" class="col-form-label"></label>
        <input asp-for="Age" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label asp-for="WorkData.Salary" class="col-form-label"></label>
        <input asp-for="WorkData.Salary" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label asp-for="WorkData.Dateofcontract" class="col-form-label"></label>
        <input asp-for="WorkData.Dateofcontract" class="form-control" readonly />
    </div>

    <br />
    <input type="submit" value="Delete" class="btn btn-danger" />

</form>


